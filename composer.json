{
	"name": "tejas/tejascaptcha",
	"type": "package",
	"description": "Laravel 5 TejasCaptcha Package",
	"keywords": ["laravel", "laravel5", "laravel 5", "captcha", "tejascaptcha", "Captcha", "TejasCaptcha", "security"],
	"homepage": "https://github.com/1007tejas/tejascaptcha",
	"license": "MIT",
	"authors": [
		{
			"name": "Jeff Hallmark",
			"email": "jhallmark1007@gmail.com",
			"homepage": "https://github.com/1007tejas",
			"role": "Developer"
		}
	],
	"require": {
		"php": ">=5.4",
		"ext-gd": "*",
		"illuminate/config": "~5.0",
		"illuminate/filesystem": "~5.0",
		"illuminate/support": "~5.0",
		"illuminate/hashing": "~5.0",
		"intervention/image": "~2.2"
	},
	"require-dev": {
		"phpunit/phpunit": "~4.1",
        	"mockery/mockery": "0.9.*"
	},
	"autoload": {
		"psr-4": {
			"Tejas\\TejasCaptcha\\": "src/"
		},
		"files": [
			"src/helpers.php"
		]
	},
	"autoload-dev": {
		"classmap": [
			"tests"
		],
		"psr-4": {
			"Tejas\\Tests\\": "tests/"
		}
	},
	"minimum-stability": "dev",
	"extra": {
		"laravel": {
			"providers": [
				"Tejas\\TejasCaptcha\\TejasCaptchaServiceProvider"
			],
			"aliases": {
				"TejasCaptcha": "Tejas\\TejasCaptcha\\Facades\\TejasCaptcha"
			}
		}
	}
}
